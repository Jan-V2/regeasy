<li v-if="model.display">
  % :class="{bold: isFolder}" @click="toggle" =>
  <div>
    {{model.type+ ": " + model.text }}
    % v-if="isFolder" =>
    <span>[{{ open ? '-' : '+' }}]</span>
  </div>
  <ol v-show="open" v-if="isFolder">
    % v-for="(model, index) in model.children" :key="index" :model="model" =>
    <item class="item">
    </item>
  </ol>
</li>
